{
    "name": "pickles2/lib-px2-module-editor",
    "description": "Pickles 2 module editor library.",
    "keywords": ["pickles2"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tomoya Koyanagi",
            "email": "tomk79@gmail.com"
        }
    ],
    "scripts":{
        "start": [
            "php -S 127.0.0.1:8088 -t ./"
        ],
        "preview": [
            "open http://127.0.0.1:8088/tests/app/client_php/"
        ],
        "test": [
            "php ./vendor/phpunit/phpunit/phpunit"
        ]
    } ,
    "config": {
        "process-timeout": 0
    },
    "minimum-stability": "dev",
    "repositories": [
    ],
    "require": {
        "php": ">=5.4.0",
        "tomk79/langbank": "^0.0.3",
        "twig/twig": "^1.35",
        "pickles2/lib-px2-contents-editor": "^2.0.5"
    },
    "require-dev": {
        "broccoli-html-editor/broccoli-module-fess": "dev-develop",
        "broccoli-html-editor/broccoli-module-plain-html-elements": "dev-develop",
        "pickles2/px-fw-2.x": "dev-develop",
        "pickles2/px2-dec": "dev-master",
        "pickles2/px2-multitheme": "dev-develop",
        "pickles2/px2-path-resolver": "dev-develop",
        "pickles2/px2-px2dthelper": "^2.0.7",
        "pickles2/px2-remove-attr": "dev-develop",
        "pickles2/px2-sitemapexcel": "dev-develop",
        "phpunit/phpunit": "^4.1.0"
    },
    "autoload": {
        "files": [
            "php/main.php",
            "php/gpi.php"
        ]
    }
}
